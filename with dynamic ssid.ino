  Serial.print(":");
  Serial.println(secondStr);

  String dateStr = dayStr + String(" ") + monthStr + String(",") + yearStr;
  char dateArray[dateStr.length() + 1];  // +1 for null terminator
  dateStr.toCharArray(dateArray, sizeof(dateArray));

  String timeStr = hoursStr + String(":") + minuteStr + String(":") + secondStr + ampmStr;
  char timeArray[timeStr.length() + 1];  // +1 for null terminator
  timeStr.toCharArray(timeArray, sizeof(timeArray));

  char dayArray[dayOfWeekStr.length() + 1];
  dayOfWeekStr.toCharArray(dayArray, sizeof(dayArray));

  u8g2.firstPage();
  do {
    // Set font for time
    u8g2.setFont(u8g2_font_ncenB14_tr);
    // Calculate x-coordinate for center alignment
    int16_t timeWidth = u8g2.getStrWidth(timeArray);
    int16_t timeX = (u8g2.getWidth() - timeWidth) / 2;
    u8g2.drawStr(timeX, 20, timeArray);

    // Set font for date
    u8g2.setFont(u8g2_font_ncenB12_tr);
    // Calculate x-coordinate for center alignment
    int16_t dateWidth = u8g2.getStrWidth(dateArray);
    int16_t dateX = (u8g2.getWidth() - dateWidth) / 2;
    u8g2.drawStr(dateX, 38, dateArray);

    // Set font for day
    u8g2.setFont(u8g2_font_ncenB10_tr);
    // Calculate x-coordinate for center alignment
    int16_t dayWidth = u8g2.getStrWidth(dayArray);
    int16_t dayX = (u8g2.getWidth() - dayWidth) / 2;
    u8g2.drawStr(dayX, 55, dayArray);
  } while ( u8g2.nextPage() );


  delay(1000);
}


