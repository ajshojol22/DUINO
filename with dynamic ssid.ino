void setup() {
  // Initialize Serial Monitor
  u8g2.begin();

  Serial.begin(115200);

  
  EEPROM.begin(512); //Initialasing EEPROM
  delay(10);
  for (int i = 0; i < 32; ++i)
  {
    esid += char(EEPROM.read(i));
  }
  Serial.println();
  Serial.print("SSID: ");
  Serial.println(esid);
  Serial.println("Reading EEPROM pass");

  for (int i = 32; i < 96; ++i)
  {
    epass += char(EEPROM.read(i));
  }


  WiFi.begin(esid.c_str(), epass.c_str());
  timestart = millis();

  Serial.println(timestart);

  while ((millis() - timestart < 60000) && WiFi.status() != WL_CONNECTED) {
    u8g2.clearBuffer();         // clear the internal memory
    u8g2.drawXBMP(39, 7, 50, 50, epd_bitmap_allArray[counter]); // draw frame of the animation
    u8g2.sendBuffer();          // transfer internal memory to the display
    counter = (counter + 1) % 28; // increase the counter, but always go between 0-27
    Serial.println(millis() - timestart);

  }

  if (WiFi.status() != WL_CONNECTED && WiFi.status() != WL_NO_SSID_AVAIL)
  {
    Serial.println("Password is not correct");


    Serial.println("Turning the HotSpot On");
    launchWeb();
    setupAP();// Setup HotSpot

    while ((WiFi.status() != WL_CONNECTED))
    {
      Serial.print(".");
      delay(100);
      server.handleClient();
    }
  }


  // Print local IP address and start web server
  Serial.println("");
  Serial.println("WiFi connected.");
  Serial.println("IP address: ");
  Serial.println(WiFi.localIP());
  timeClient.begin();
  timeClient.setTimeOffset(21600);

}

String getTimeStampString() {
  time_t rawtime = timeClient.getEpochTime();
  struct tm * ti;
  ti = localtime (&rawtime);

  uint16_t year = ti->tm_year + 1900;
  yearStr = String(year);

  uint8_t month = ti->tm_mon + 1;

  switch (month) {
    case 1: monthStr = "Jan"; break;
    case 2: monthStr = "Feb"; break;
    case 3: monthStr = "Mar"; break;
    case 4: monthStr = "Apr"; break;
    case 5: monthStr = "May"; break;
    case 6: monthStr = "June"; break;
    case 7: monthStr = "Jul"; break;
    case 8: monthStr = "Aug"; break;
    case 9: monthStr = "Sept"; break;
    case 10: monthStr = "Oct"; break;
    case 11: monthStr = "Nov"; break;
    case 12: monthStr = "Dec"; break;
    default: monthStr = "NAN"; break;
  }

  //monthStr = month < 10 ? "0" + String(month) : String(month);

  uint8_t day = ti->tm_mday;
  dayStr = day < 10 ? "0" + String(day) : String(day);

  uint8_t hours = ti->tm_hour;
  ampmStr = hours < 12 ? " AM" : " PM";

  hours = hours % 12; // Convert to 12-hour format
  Serial.println(hoursStr);
  hoursStr = hours < 10 ? "0" + String(hours) : String(hours);
  hoursStr = hoursStr == 0 ? "12" : String(hoursStr);


  uint8_t minutes = ti->tm_min;
  minuteStr = minutes < 10 ? "0" + String(minutes) : String(minutes);

  uint8_t seconds = ti->tm_sec;
  secondStr = seconds < 10 ? "0" + String(seconds) : String(seconds);

  uint8_t dayOfWeek = ti->tm_wday;
  switch (dayOfWeek) {
    case 0: dayOfWeekStr = "Sunday"; break;
    case 1: dayOfWeekStr = "Monday"; break;
    case 2: dayOfWeekStr = "Tuesday"; break;
    case 3: dayOfWeekStr = "Wednesday"; break;
    case 4: dayOfWeekStr = "Thursday"; break;
    case 5: dayOfWeekStr = "Friday"; break;
    case 6: dayOfWeekStr = "Saturday"; break;
    default: dayOfWeekStr = "Invalid Day"; break;
  }


}

void loop() {
  timeClient.update();
  getTimeStampString();
  Serial.print(dayStr);
  Serial.print("-");

  Serial.print(monthStr);
  Serial.print("-");
